{
  "name": "@techpotli/cart-service",
  "version": "1.0.0",
  "description": "Shopping cart management service for TechPotli e-commerce platform",
  "author": "TechPotli Team",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/config/database.config.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/config/database.config.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/config/database.config.ts",
    "schema:sync": "npm run typeorm -- schema:sync -d src/config/database.config.ts",
    "schema:drop": "npm run typeorm -- schema:drop -d src/config/database.config.ts",
    "seed": "ts-node src/database/seeds/seed.ts",
    "docker:build": "docker build -t techpotli/cart-service:latest .",
    "docker:push": "docker push techpotli/cart-service:latest",
    "docker:run": "docker run -p 3005:3005 techpotli/cart-service:latest",
    "k8s:deploy": "kubectl apply -f k8s/",
    "k8s:delete": "kubectl delete -f k8s/",
    "health:check": "curl -f http://localhost:3005/health || exit 1",
    "metrics:check": "curl -f http://localhost:3005/metrics || exit 1"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/bull": "^10.0.0",
    "@nestjs/throttler": "^5.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/terminus": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/microservices": "^10.0.0",
    "typeorm": "^0.3.17",
    "mysql2": "^3.6.0",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-store": "^3.0.1",
    "redis": "^4.6.7",
    "bull": "^4.11.0",
    "kafkajs": "^2.2.4",
    "uuid": "^9.0.0",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express-rate-limit": "^6.10.0",
    "express-slow-down": "^1.6.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "joi": "^17.9.2",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "axios": "^1.5.0",
    "node-cron": "^3.0.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/uuid": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/node-cron": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^46.4.4",
    "eslint-plugin-prefer-arrow": "^1.3.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.1",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/techpotli/backend.git",
    "directory": "services/cart-service"
  },
  "keywords": [
    "nestjs",
    "cart",
    "ecommerce",
    "microservice",
    "typescript",
    "shopping",
    "basket",
    "checkout"
  ],
  "bugs": {
    "url": "https://github.com/techpotli/backend/issues"
  },
  "homepage": "https://github.com/techpotli/backend#readme"
}
